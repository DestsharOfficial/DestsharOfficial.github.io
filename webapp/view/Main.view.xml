<mvc:View controllerName="com.destshar.cv.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout">
    <Shell id="shell" appWidthLimited="true">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}" showHeader="false">
                    <!-- <l:Splitter>
                        <Text text="Content 1">
                            <layoutData>
                                <l:SplitterLayoutData size="30%" resizable="false" />
                            </layoutData>
                        </Text>
                        <Text text="Content 2">
                            <layoutData>
                                <l:SplitterLayoutData size="auto" />
                            </layoutData>
                        </Text>
                    </l:Splitter> -->
                    <l:Grid>
                        <VBox alignContent="Center">
                            <layoutData>
                                <l:GridData span="XL3 L3 M3 S12"/>
                            </layoutData>
                            <items>
                                <Panel headerText="Skills">
                                    <l:Grid defaultSpan="XL6 L6 M6 S12">
                                        <Text text="SAPUI5"/>
                                        <RatingIndicator maxValue="5" value="4" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="JavaScript"/>
                                        <RatingIndicator maxValue="5" value="4" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="ABAP"/>
                                        <RatingIndicator maxValue="5" value="4" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="OData"/>
                                        <RatingIndicator maxValue="5" value="3" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="CDS"/>
                                        <RatingIndicator maxValue="5" value="1" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="WebDynpro"/>
                                        <RatingIndicator maxValue="5" value="3" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="SmartForms"/>
                                        <RatingIndicator maxValue="5" value="3" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="SAP HCM"/>
                                        <RatingIndicator maxValue="5" value="4" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="Selenium"/>
                                        <RatingIndicator maxValue="5" value="2" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                    </l:Grid>
                                </Panel>
                                <Panel headerText="Languages">
                                    <l:Grid defaultSpan="XL6 L6 M6 S12">
                                        <Text text="English"/>
                                        <RatingIndicator maxValue="5" value="5" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="German"/>
                                        <RatingIndicator maxValue="5" value="2" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="French"/>
                                        <RatingIndicator maxValue="5" value="1" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                        <Text text="Russian"/>
                                        <RatingIndicator maxValue="5" value="1" iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" displayOnly="true"/>
                                    </l:Grid>
                                    <!-- <HBox>
                                            <Text text="German"/>
                                            <RatingIndicator iconUnselected="sap-icon://circle-task" iconSelected="sap-icon://circle-task-2" maxValue="5" value="2" displayOnly="true"/>
                                        </HBox>-->
                                </Panel>
                                <Panel headerText="Intrests">
                                    <l:Grid defaultSpan="XL12 L12 M12 S12">
                                        <Text text="English"/>
                                        <Text text="German"/>
                                    </l:Grid>
                                </Panel>
                            </items>
                        </VBox>
                        <!-- <VBox backgroundDesign="Solid"> -->
                        <VBox>
                            <layoutData>
                                <l:GridData span="XL8 L8 M8 S12"/>
                            </layoutData>
                            <items>
                                <Panel headerText="Experience">

                                </Panel>
                            </items>
                        </VBox>
                    </l:Grid>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
